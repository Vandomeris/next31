// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model users {
  id      Int    @id @default(autoincrement())
  name    String
  surname String
  age     Int
  email   String
}

model products {
  id          Int    @id @default(autoincrement())
  name        String
  price       Float
  description String
}

model rooms {
  id          Int    @id @default(autoincrement())
  name        String
  peopleMax   Int
  address     String
  price       Int
  description String
}
